<?xml version="1.0"?>
<robot name="hand">

<!-- Material colors -->
<material name="blue">
    <color rgba="0 0 0.8 1"/>
</material>
<material name="green">
    <color rgba="0.0 0.8 0.0 1.0"/>
</material>
<material name="grey">
    <color rgba="0.2 0.2 0.2 1.0"/>
</material>

<!--The palm -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.6 0.3 0.8"/>
      </geometry>
    </visual>
  </link>

<!-- First Finger -->
  <link name="first_finger_01">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.05"/>
      </geometry>
     <material name="blue"/>
    <origin rpy="0 0 0" xyz="0 0.0 0.1"/>
    </visual>
  </link>

  <joint name="base_to_first_finger_01" type="revolute">
    <parent link="base_link"/>
    <child link="first_finger_01"/>
    <axis xyz="1 0 0"/>
    <origin xyz="-0.25 0 0.4"/>
    <limit effort ="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
  </joint>

